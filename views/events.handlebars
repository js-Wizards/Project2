<div class="uk-container-large">
    <nav class="uk-navbar-container uk-margin" uk-navbar>
        <div class="uk-navbar-left">
            <a class="uk-navbar-item uk-logo" href="#">EventFinder</a>
        </div>
        <div class="uk-navbar-right">

            <ul class="uk-navbar-nav">
                <li class="uk-active"><a href="#">Welcome:' '</a></li>
                <li>
                    <a href="#"></a>
                    <div class="uk-navbar-dropdown">
                    </div>
                </li>
                <li><a></a></li>
            </ul>

        </div>
    </nav>
    <div uk-grid>
        <div id="public-events" class="uk-width-2-3 uk-box-shadow-small uk-padding uk-clearfix">
            <h3 class="uk-card-title uk-text-center">Events In Your Area</h3>
            <ul>
                {{#each eventsDB}}
                <div class="uk-card uk-card-default uk-width-1-2@m uk-align-center">
                    <div class="uk-card-header">
                        <div class="uk-grid-small uk-flex-middle" uk-grid>
                            <div class="uk-width-expand">
                                <h3 class="uk-card-title uk-margin-remove-bottom">{{eventName}}</h3>
                                <p class="uk-text-meta uk-margin-remove-top"></p>
                            </div>
                        </div>
                    </div>
                    <div class="uk-card-body">
                        <p>{{startDate}} {{startTime}}</p>
                        <p>{{venueName}} {{addressLine1}} {{addressLine2}} {{city}} {{state}}
                            {{postalCode}}</p>
                    </div>
                    <div class="uk-card-footer">
                        <a href="#" class="uk-button uk-button-text">Add To My events</a>
                    </div>
                </div>
                {{/each}}
            </ul>
        </div>

        <div id="my-events" class="uk-width-1-3 uk-box-shadow-small uk-padding uk-clearfix">
            <h3 class="uk-card-title uk-text-center">My Events</h3>
            <div class="uk-card uk-card-default uk-card-body">
                <div class=>
                </div>
            </div>
        </div>
    </div>
</div>